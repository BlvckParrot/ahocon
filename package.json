{
  "name": "ahocon",
  "version": "0.2.5",
  "scripts": {
    "dev": "vite --mode=extended",
    "dev-lite": "vite --mode=lite",
    "build": "sh build.sh",
    "test": "vitest",
    "test-extended": "vitest --mode=extended",
    "test:ui": "vitest",
    "postinstall": "husky install",
    "format": "pretty-quick",
    "prepublish": "pnpm run test",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@vitest/ui": "^0.25.8",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "pinst": "^3.0.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vite-plugin-remove-exports": "^1.1.0",
    "vitest": "^0.25.8"
  },
  "main": "./index.umd.js",
  "module": "./index.es.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.es.js",
      "require": "./index.umd.js"
    },
    "./extended": {
      "import": "./extended/index.es.js",
      "require": "./extended/index.umd.js"
    },
    "./ast": {
      "import": "./ast.es.js",
      "require": "./ast.umd.js"
    },
    "./extended/ast": {
      "import": "./extended/ast.es.js",
      "require": "./extended/ast.umd.js"
    },
    "./input": {
      "import": "./input.es.js",
      "require": "./input.umd.js"
    },
    "./extended/input": {
      "import": "./extended/input.es.js",
      "require": "./extended/input.umd.js"
    },
    "./funcs/ref": {
      "import": "./funcs/ref.es.js",
      "require": "./funcs/ref.umd.js"
    },
    "./funcs/math": {
      "import": "./funcs/math.es.js",
      "require": "./funcs/math.umd.js"
    },
    "./funcs/convert": {
      "import": "./funcs/convert.es.js",
      "require": "./funcs/convert.umd.js"
    }
  },
  "description": "a hocon superset",
  "author": "steve_py96",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/steve-py96/ahocon"
  },
  "homepage": "https://steve-py96.github.io/ahocon/",
  "keywords": [
    "ahocon",
    "hocon",
    "json",
    "config",
    "simple"
  ]
}
